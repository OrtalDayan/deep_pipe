model_core = model_core:deepmedic_feat
    with_dropout = false
    downsampling_type = "sampling"
    n_chans_in = n_chans_in
    n_chans_out = n_chans_out
    n_features = n_quantiles

model = model:model
    model_core = model_core
    logits2pred = logits2pred
    logits2loss = logits2loss
    optimize = optimize:tf_optimize
        @lazy
        tf_optimizer_name = "MomentumOptimizer"
        use_nesterov = true
        momentum = 0.6

frozen_model = model:frozen_model
    model_core = model_core
    logits2pred = logits2pred
    restore_model_path = restore_model_path

batch_predict = batch_predict:patch_3d_fixed_quantiles
    x_patch_sizes = [[106, 106, 106], [138, 138, 138]]
    y_patch_size = [90, 90, 90]
    n_quantiles = n_quantiles
    padding_mode = "min"
